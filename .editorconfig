# EditorConfig: https://EditorConfig.org
# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = tab
# sets also tab_width
indent_size = 2

# Set default charset
[*.{h,cpp}]
charset = utf-8
trim_trailing_whitespace = true

